{
    "CPUCommitment": 1,
    "Command": "su - zenoss -c \"/opt/zenoss/bin/zenjserver run -c --logfileonly \"",
    "ConfigFiles": {
        "/opt/zenoss/etc/global.conf": {
            "Content": "# Generated by serviced\n{{range $k,$v:=contextFilter . \"global.conf.\"}}{{$k}} {{$v}}\n{{end}}",
            "Filename": "/opt/zenoss/etc/global.conf",
            "Owner": "zenoss:zenoss",
            "Permissions": "660"
        },
        "/opt/zenoss/etc/zenjserver.conf": {
            "FileName": "/opt/zenoss/etc/zenjserver.conf",
            "Owner": "zenoss:zenoss",
            "Permissions": "0664"
        }
    },
    "Description": "Enables the dynamic view of organizers",
    "Endpoints": [
        {
            "Application": "zproxy",
            "Name": "zproxy",
            "PortNumber": 8080,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "localhost_zenhubPB",
            "Name": "zenhub",
            "PortNumber": 8789,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "redis",
            "Name": "redis",
            "PortNumber": 6379,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "controlplane_consumer",
            "Name": "controlplane_consumer",
            "PortNumber": 8444,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "dynamicserver",
            "Name": "dynamicserver",
            "PortNumber": 8700,
            "Protocol": "tcp",
            "Purpose": "export"
        }
    ],
    "HealthChecks": {
        "heartbeat": {
            "Interval": 5.0,
            "Script": "echo heartbeat"
        }
    },
    "ImageID": "zenoss/zenoss5x",
    "Instances": {
        "min": 1
    },
    "Launch": "auto",
    "LogConfigs": [
        {
            "filters": [
                "pythondaemon"
            ],
            "path": "/opt/zenoss/log/zenjserver.log",
            "type": "zenjserver"
        },
        {
            "filters": [
                "supervisord"
            ],
            "isAudit": true,
            "path": "/opt/zenoss/log/audit.log",
            "type": "zenossaudit"
        }
    ],
    "Name": "zenjserver",
    "RAMCommitment": "256M",
    "Services": [],
    "Tags": [
        "daemon"
    ],
    "Volumes": [
        {
            "ContainerPath": "/home/zenoss/.ssh",
            "Owner": "zenoss:zenoss",
            "Permission": "0755",
            "ResourcePath": ".ssh",
            "Type": ""
        },
        {
            "ContainerPath": "/var/zenoss",
            "Owner": "zenoss:zenoss",
            "Permission": "0775",
            "ResourcePath": "var-zenpacks"
        }
    ]
}
