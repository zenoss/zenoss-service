{
    "CPUCommitment": 4,
    "Command": "/bin/supervisord -n -c /etc/mariadb/mariadb_supervisor.conf",
    "ConfigFiles": {
        "/etc/mariadb/mariadb_supervisor.conf": {
            "FileName": "/etc/mariadb/mariadb_supervisor.conf",
            "Owner": "root:root",
            "Permissions": "0664"
        },
        "/opt/zenoss/etc/global.conf": {
            "Content": "# Generated by serviced\n{{range $k,$v:=contextFilter . \"global.conf.\"}}{{$k}} {{$v}}\n{{end}}",
            "Filename": "/opt/zenoss/etc/global.conf",
            "Owner": "zenoss:zenoss",
            "Permissions": "660"
        }
    },
    "Description": "MariaDB events database server",
    "EmergencyShutdownLevel": 1,
    "Endpoints": [
        {
            "Application": "zep_mariadb",
            "Name": "zep_mariadb",
            "PortNumber": 3306,
            "Protocol": "tcp",
            "Purpose": "export"
        }
    ],
    "Environment": [
        "MARIADB_DB=zep"
    ],
    "HealthChecks": {
        "answering": {
            "Interval": 5.0,
            "Script": "mysql --protocol TCP -u{{(getContext . \"global.conf.zep-user\")}} -h{{(getContext . \"global.conf.zep-host\")}} -P{{(getContext . \"global.conf.zep-port\")}} -p{{getContext . \"global.conf.zep-password\"}} -e 'select 1' > /dev/null"
        }
    },
    "ImageID": "zenoss/mariadb:xx",
    "Instances": {
        "max": 1,
        "min": 1
    },
    "Launch": "auto",
    "LogConfigs": [
        {
            "filters": [
                "mariadb"
            ],
            "path": "/var/log/mysqld.log",
            "type": "mariadb"
        }
    ],
    "Name": "mariadb-events",
    "OomKillDisable": true,
    "OomScoreAdj": 0,
    "PIDFile": "",
    "RAMCommitment": "2G",
    "Snapshot": {
        "Pause": "PAUSE_CHECK_TIMEOUT=60 LOCK_HOLD_DURATION=600 ${ZENHOME:-/opt/zenoss}/bin/quiesce-mariadb.sh pause",
        "Resume": "${ZENHOME:-/opt/zenoss}/bin/quiesce-mariadb.sh resume"
    },
    "StartLevel": 1,
    "Tags": [
        "daemon"
    ],
    "Volumes": [
        {
            "#####": "drwxr-xr-x 12 mysql mysql 4096 Feb 25 21:27 /var/lib/mysql/",
            "ContainerPath": "/var/lib/mysql",
            "Owner": "mysql:mysql",
            "Permission": "0755",
            "ResourcePath": "mariadb-events"
        }
    ]
}
