{
    "Actions": {
        "debug": "su - zenoss -c '${ZENHOME:-/opt/zenoss}/bin/{{.Name}} debug'",
        "stats": "su - zenoss -c '${ZENHOME:-/opt/zenoss}/bin/{{.Name}} stats'"
    },
    "CPUCommitment": 1,
    "ChangeOptions": [
        "restartAllOnInstanceChanged"
    ],
    "Command": "su - zenoss -c \"/opt/zenoss/bin/zenmodeler run -c --logfileonly --workers {{.Instances}} --workerid $CONTROLPLANE_INSTANCE_ID --monitor {{(parent .).Name}} \"",
    "ConfigFiles": {
        "/opt/zenoss/etc/global.conf": {
            "Content": "# Generated by serviced\n{{range $k,$v:=contextFilter . \"global.conf.\"}}{{$k}} {{$v}}\n{{end}}",
            "Filename": "/opt/zenoss/etc/global.conf",
            "Owner": "zenoss:zenoss",
            "Permissions": "660"
        },
        "/opt/zenoss/etc/zenmodeler.conf": {
            "FileName": "/opt/zenoss/etc/zenmodeler.conf",
            "Owner": "zenoss:zenoss",
            "Permissions": "0664"
        }
    },
    "Description": "Configuration collection and configuration daemon, used for high-performance, automated model population",
    "Endpoints": [
        {
            "Application": "zproxy",
            "Name": "zproxy",
            "PortNumber": 8080,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "{{(parent (parent .)).Name}}_zenhubPB",
            "Name": "zenhub",
            "PortNumber": 8789,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "{{(parent .).Name}}_redis",
            "Name": "CollectorRedis",
            "PortNumber": 6379,
            "Protocol": "tcp",
            "Purpose": "import_all",
            "VirtualAddress": "rd1:6379"
        },
        {
            "Application": "controlplane_consumer",
            "Name": "controlplane_consumer",
            "PortNumber": 8444,
            "Protocol": "tcp",
            "Purpose": "import"
        }
    ],
    "HealthChecks": {
        "redis_answering": {
            "Interval": 5.0,
            "Script": "/opt/zenoss/bin/healthchecks/redis_answering"
        },
        "running": {
            "Interval": 5.0,
            "Script": "pgrep -fu zenoss zenmodeler.py > /dev/null"
        },
        "zenhub_answering": {
            "Interval": 10.0,
            "Script": "/opt/zenoss/bin/healthchecks/zenhub_answering"
        }
    },
    "ImageID": "zenoss/zenoss5x",
    "Instances": {
        "min": 1
    },
    "Launch": "auto",
    "LogConfigs": [
        {
            "LogTags": [
                {
                    "Name": "monitor",
                    "Value": "{{(parent .).Name}}"
                }
            ],
            "filters": [
                "pythondaemon"
            ],
            "path": "/opt/zenoss/log/zenmodeler.log",
            "type": "zenmodeler"
        },
        {
            "filters": [
                "supervisord"
            ],
            "isAudit": true,
            "path": "/opt/zenoss/log/audit.log",
            "type": "zenossaudit"
        }
    ],
    "MonitoringProfile": {
        "GraphConfigs": [],
        "MetricConfigs": [
            {
                "Description": "zenmodeler internal metrics",
                "ID": "zenmodeler",
                "Metrics": [
                    {
                        "Counter": false,
                        "Description": "Daemon collection interval",
                        "ID": "cycleTime",
                        "Name": "Cycle Time",
                        "Unit": "Time"
                    },
                    {
                        "Counter": true,
                        "Description": "Number of devices since daemon startup.",
                        "ID": "modeledDevices",
                        "Name": "Modeled Devices",
                        "Unit": "Devices"
                    },
                    {
                        "Counter": false,
                        "Description": "Number of devices currently modeled.",
                        "ID": "modeledDevicesCount",
                        "Name": "Modeled Devices Count",
                        "Unit": "Devices"
                    }
                ],
                "Name": "zenmodeler internal metrics"
            }
        ]
    },
    "Name": "zenmodeler",
    "RAMCommitment": "256M",
    "Services": [],
    "Tags": [
        "daemon",
        "collector"
    ],
    "Volumes": [
        {
            "ContainerPath": "/home/zenoss/.ssh",
            "Owner": "zenoss:zenoss",
            "Permission": "0755",
            "ResourcePath": ".ssh",
            "Type": ""
        },
        {
            "ContainerPath": "/var/zenoss",
            "Owner": "zenoss:zenoss",
            "Permission": "0775",
            "ResourcePath": "var-zenpacks"
        }
    ]
}