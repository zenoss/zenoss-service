{
    "Actions": {
        "debug": "su - zenoss -c '${ZENHOME:-/opt/zenoss}/bin/{{.Name}} debug'",
        "stats": "su - zenoss -c '${ZENHOME:-/opt/zenoss}/bin/{{.Name}} stats'"
    },
    "CPUCommitment": 1,
    "Command": "su - zenoss -c \"/opt/zenoss/bin/zentrap run -c --logfileonly --monitor {{(parent .).Name}} \"",
    "ConfigFiles": {
        "/opt/zenoss/etc/global.conf": {
            "Content": "# Generated by serviced\n{{range $k,$v:=contextFilter . \"global.conf.\"}}{{$k}} {{$v}}\n{{end}}",
            "Filename": "/opt/zenoss/etc/global.conf",
            "Owner": "zenoss:zenoss",
            "Permissions": "660"
        },
        "/opt/zenoss/etc/zentrap.conf": {
            "FileName": "/opt/zenoss/etc/zentrap.conf",
            "Owner": "zenoss:zenoss",
            "Permissions": "0664"
        },
        "/opt/zenoss/etc/zentrap.filter.conf": {
            "FileName": "/opt/zenoss/etc/zentrap.filter.conf",
            "Owner": "zenoss:zenoss",
            "Permissions": "0664"
        }
    },
    "Description": "Collects and parses SNMP traps, resolves OIDs into MIB names, and then forwards the traps to zenhub for additional processing",
    "Endpoints": [
        {
            "Application": "zproxy",
            "Name": "zproxy",
            "PortNumber": 8080,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "{{(parent (parent .)).Name}}_zenhubPB",
            "Name": "zenhub",
            "PortNumber": 8789,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "{{(parent .).Name}}_redis",
            "Name": "CollectorRedis",
            "PortNumber": 6379,
            "Protocol": "tcp",
            "Purpose": "import_all"
        },
        {
            "Application": "controlplane_consumer",
            "Name": "controlplane_consumer",
            "PortNumber": 8444,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "AddressConfig": {
                "Port": 162,
                "Protocol": "udp"
            },
            "Application": "{{(parent .).Name}}_snmp_trap",
            "Name": "zentrap",
            "PortNumber": 162,
            "Protocol": "udp",
            "Purpose": "export"
        }
    ],
    "HealthChecks": {
        "listening": {
            "Interval": 10.0,
            "Script": "nmap -oG - -Pn -sU -p 162 127.0.0.1 | grep -q 'Ports: 162'"
        },
        "redis_answering": {
            "Interval": 5.0,
            "Script": "/opt/zenoss/bin/healthchecks/redis_answering"
        },
        "running": {
            "Interval": 5.0,
            "Script": "pgrep -fu zenoss zentrap.py > /dev/null"
        },
        "zenhub_answering": {
            "Interval": 10.0,
            "Script": "/opt/zenoss/bin/healthchecks/zenhub_answering"
        }
    },
    "ImageID": "zenoss/zenoss5x",
    "Instances": {
        "max": 1,
        "min": 1
    },
    "Launch": "auto",
    "LogConfigs": [
        {
            "LogTags": [
                {
                    "Name": "monitor",
                    "Value": "{{(parent .).Name}}"
                }
            ],
            "filters": [
                "pythondaemon"
            ],
            "path": "/opt/zenoss/log/zentrap.log",
            "type": "zentrap"
        },
        {
            "filters": [
                "supervisord"
            ],
            "isAudit": true,
            "path": "/opt/zenoss/log/audit.log",
            "type": "zenossaudit"
        }
    ],
    "MonitoringProfile": {
        "GraphConfigs": [
            {
                "datapoints": [
                    {
                        "aggregator": "avg",
                        "fill": false,
                        "format": "%d",
                        "id": "cycleTime",
                        "legend": "Cycle Time",
                        "metric": "cycleTime",
                        "metricSource": "zentrap",
                        "name": "Cycle Time",
                        "rate": false,
                        "type": "line"
                    }
                ],
                "description": "Daemon collection interval.",
                "footer": false,
                "id": "cycleTime",
                "name": "Cycle Time",
                "range": {
                    "end": "0s-ago",
                    "start": "1h-ago"
                },
                "returnset": "EXACT",
                "type": "line",
                "yAxisLabel": "Cycle Time"
            },
            {
                "datapoints": [
                    {
                        "aggregator": "avg",
                        "fill": false,
                        "format": "%d",
                        "id": "devices",
                        "legend": "Devices",
                        "metric": "devices",
                        "metricSource": "zentrap",
                        "name": "Devices",
                        "rate": false,
                        "type": "line"
                    }
                ],
                "description": "Number of devices.",
                "footer": false,
                "id": "devices",
                "name": "Devices",
                "range": {
                    "end": "0s-ago",
                    "start": "1h-ago"
                },
                "returnset": "EXACT",
                "type": "line",
                "yAxisLabel": "Devices"
            },
            {
                "datapoints": [
                    {
                        "aggregator": "avg",
                        "fill": false,
                        "format": "%d",
                        "id": "eventQueueLength",
                        "legend": "Event Queue Length",
                        "metric": "eventQueueLength",
                        "metricSource": "zentrap",
                        "name": "Event Queue Length",
                        "rate": false,
                        "type": "line"
                    }
                ],
                "description": "The number of events pending to be flushed from a daemon's memory queue.",
                "footer": false,
                "id": "eventQueueLength",
                "name": "Event Queue Length",
                "range": {
                    "end": "0s-ago",
                    "start": "1h-ago"
                },
                "returnset": "EXACT",
                "type": "line",
                "yAxisLabel": "Event Queue Length"
            }
        ],
        "MetricConfigs": [
            {
                "Description": "zentrap internal metrics",
                "ID": "zentrap",
                "Metrics": [
                    {
                        "Counter": true,
                        "Description": "Number of datapoints collected.",
                        "ID": "dataPoints",
                        "Name": "Datapoints",
                        "Unit": "Points"
                    },
                    {
                        "Counter": false,
                        "Description": "Number of devices.",
                        "ID": "devices",
                        "Name": "Devices"
                    },
                    {
                        "Counter": true,
                        "Description": "Total number of events.",
                        "ID": "eventCount",
                        "Name": "Event Count"
                    },
                    {
                        "Counter": false,
                        "Description": "The number of events pending to be flushed from a daemon's memory queue.",
                        "ID": "eventQueueLength",
                        "Name": "Event Queue Length"
                    },
                    {
                        "Counter": true,
                        "Description": "Number of missed runs of tasks running on daemon.",
                        "ID": "missedRuns",
                        "Name": "Missed Runs"
                    },
                    {
                        "Counter": false,
                        "Description": "Number of tasks scheduled to be run, but are not running yet.",
                        "ID": "queuedTasks",
                        "Name": "Queued Tasks"
                    },
                    {
                        "Counter": false,
                        "Description": "Number of currently running tasks.",
                        "ID": "runningTasks",
                        "Name": "Running Tasks"
                    },
                    {
                        "Counter": false,
                        "Description": "Total number of tasks.",
                        "ID": "taskCount",
                        "Name": "Task Count"
                    },
                    {
                        "Counter": false,
                        "Description": "Daemon collection interval",
                        "ID": "cycleTime",
                        "Name": "Cycle Time",
                        "Unit": "Time"
                    },
                    {
                        "Counter": true,
                        "Descritption": "Number of events sent by a daemon to the raw event queue.",
                        "ID": "events",
                        "Name": "Events"
                    }
                ],
                "Name": "zentrap internal metrics"
            }
        ]
    },
    "Name": "zentrap",
    "RAMCommitment": "256M",
    "Services": [],
    "Tags": [
        "daemon",
        "collector"
    ],
    "Volumes": [
        {
            "ContainerPath": "/home/zenoss/.ssh",
            "Owner": "zenoss:zenoss",
            "Permission": "0755",
            "ResourcePath": ".ssh",
            "Type": ""
        },
        {
            "ContainerPath": "/var/zenoss",
            "Owner": "zenoss:zenoss",
            "Permission": "0775",
            "ResourcePath": "var-zenpacks"
        }
    ]
}
