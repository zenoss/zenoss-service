    "Description": "Provides service-oriented alerting and root-cause analysis.",
    "EmergencyShutdownLevel": 1,
    "Endpoints": [
        {
            "Application": "redis",
            "Name": "redis",
            "PortNumber": 6379,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "zodb_impact",
            "Name": "impact",
            "PortNumber": 8083,
            "Protocol": "tcp",
            "Purpose": "export"
        },
        {
            "Application": "rabbitmq.*",
            "Name": "rabbitmq",
            "Purpose": "import"
        },
        {
            "Application": "zproxy",
            "Name": "zproxy",
            "PortNumber": 8080,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "neo4j_admin",
            "Name": "neo4j admin",
            "PortNumber": 7474,
            "Protocol": "tcp",
            "Purpose": "export"
        }
    ],
    "HealthChecks": {
        "rabbit_answering": {
            "Interval": 10.0,
            "Script": "/opt/zenoss_impact/bin/healthchecks/rabbit_answering {{(getContext . \"global.conf.amqpuser\")}} {{(getContext . \"global.conf.amqppassword\")}}"
        },
        "running": {
            "Interval": 5.0,
            "Script": "pgrep -fu zenossimpact java.*zenoss_impact >/dev/null"
        },
        "webservice-status": {
            "Interval": 5.0,
            "Script": "/opt/zenoss_impact/bin/healthchecks/impact-health-status"
        }
    },
    "ImageId": "zendev/impact-devimg",
    "Instances": {
        "Max": 1,
        "Min": 1
    },
    "Launch": "auto",
    "LogConfigs": [
        {
            "path": "/opt/zenoss_impact/log/zenoss_impact.log",
            "type": "impact"
        },
        {
            "path": "/opt/zenoss_impact/log/zenoss_impact-stdio*.log",
            "type": "impact_stdio"
        }
    ],
