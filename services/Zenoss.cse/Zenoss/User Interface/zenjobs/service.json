{
    "CPUCommitment": 1,
    "Command": "/opt/zenoss/bin/zenjobs worker -n \"zenjobs-$CONTROLPLANE_INSTANCE_ID@%h\"",
    "ConfigFiles": {
        "/opt/zenoss/etc/global.conf": {
            "Content": "# Generated by serviced\n{{range $k,$v:=contextFilter . \"global.conf.\"}}{{$k}} {{$v}}\n{{end}}",
            "Filename": "/opt/zenoss/etc/global.conf",
            "Owner": "zenoss:zenoss",
            "Permissions": "660"
        },
        "/opt/zenoss/etc/zenjobs.conf": {
            "FileName": "/opt/zenoss/etc/zenjobs.conf",
            "Owner": "zenoss:zenoss",
            "Permissions": "0664"
        },
        "/opt/zenoss/etc/zenjobs_log_levels.conf": {
            "FileName": "/opt/zenoss/etc/zenjobs_log_levels.conf",
            "Owner": "zenoss:zenoss",
            "Permissions": "0664"
        },
        "/opt/zenoss/etc/zodb.conf": {
            "FileName": "/opt/zenoss/etc/zodb.conf",
            "Owner": "zenoss:zenoss",
            "Permissions": "0660"
        }
    },
    "Description": "Performs tasks in its queue as scheduled",
    "Endpoints": [
        {
            "Application": "localhost_zenhubPB",
            "Name": "zenhub",
            "PortNumber": 8789,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "rabbitmq.*",
            "Name": "rabbitmq",
            "Purpose": "import"
        },
        {
            "Application": "zodb_.*",
            "Name": "mariadb",
            "Purpose": "import"
        },
        {
            "Application": "memcached",
            "Name": "memcached",
            "PortNumber": 11211,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "redis",
            "Name": "redis",
            "PortNumber": 6379,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "controlplane_consumer",
            "Name": "controlplane_consumer",
            "PortNumber": 8444,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "zep",
            "Name": "zep",
            "PortNumber": 8084,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "zing-connector",
            "Name": "zing-connector",
            "PortNumber": 9237,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "zproxy",
            "Name": "zproxy",
            "PortNumber": 8080,
            "Protocol": "tcp",
            "Purpose": "import"
        }
    ],
    "HealthChecks": {
        "mariadb_answering": {
            "Interval": 10.0,
            "Script": "/opt/zenoss/bin/healthchecks/mariadb_answering"
        },
        "memcached_answering": {
            "Interval": 10.0,
            "Script": "/opt/zenoss/bin/healthchecks/memcached_answering"
        },
        "rabbit_answering": {
            "Interval": 10.0,
            "Script": "/opt/zenoss/bin/healthchecks/rabbit_answering {{(getContext . \"global.conf.amqpuser\")}} {{(getContext . \"global.conf.amqppassword\")}}"
        },
        "redis_answering": {
            "Interval": 5.0,
            "Script": "/opt/zenoss/bin/healthchecks/redis_answering"
        },
        "running": {
            "Interval": 5.0,
            "Script": "pgrep -fu zenoss zenjobs > /dev/null"
        },
        "solr_answering": {
            "Interval": 10.0,
            "Script": "curl -A 'Solr answering healthcheck' -s http://localhost:8983/solr/zenoss_model/admin/ping?wt=json | grep -q '\"status\":\"OK\"'"
        },
        "stable_workers": {
            "Interval": 5.0,
            "Script": "/opt/zenoss/bin/healthchecks/zenjobs/stable_workers"
        },
        "zing-connector-answering": {
            "Interval": 10.0,
            "Script": "test \"$(/opt/zenoss/bin/healthchecks/zing_connector_answering 9237)\" = \"PONG\""
        }
    },
    "ImageID": "zenoss/zenoss5x",
    "Instances": {
        "default": 2,
        "min": 1
    },
    "Launch": "auto",
    "LogConfigs": [
        {
            "filters": [
                "pythondaemon"
            ],
            "path": "/opt/zenoss/log/zenjobs.log",
            "type": "zenjobs"
        },
        {
            "filters": [
                "supervisord"
            ],
            "isAudit": true,
            "path": "/opt/zenoss/log/audit.log",
            "type": "zenossaudit"
        }
    ],
    "Name": "zenjobs",
    "Prereqs": [
        {
            "Name": "MariaDB connectivity",
            "Script": "su - zenoss -c '/opt/zenoss/bin/python /opt/zenoss/Products/ZenUtils/ZenDB.py --usedb zodb --execsql=\";\"'"
        }
    ],
    "RAMCommitment": "1G",
    "RunAs": "zenoss",
    "Services": [],
    "Tags": [
        "daemon"
    ],
    "Volumes": [
        {
            "#####": "drwxr-xr-x 2 zenoss zenoss 4096 Feb  6 19:11 /opt/zenoss/log/jobs",
            "ContainerPath": "/opt/zenoss/log/jobs",
            "Owner": "zenoss:zenoss",
            "Permission": "0755",
            "ResourcePath": "zenjobs"
        },
        {
            "#####": "drwxrwxr-x 3 zenoss zenoss 4096 Jul 18 11:20 /opt/zenoss/var/ext",
            "ContainerPath": "/opt/zenoss/var/ext",
            "Owner": "zenoss:zenoss",
            "Permission": "0775",
            "ResourcePath": "zenoss-var-ext"
        },
        {
            "ContainerPath": "/home/zenoss/.ssh",
            "Owner": "zenoss:zenoss",
            "Permission": "0755",
            "ResourcePath": ".ssh",
            "Type": ""
        },
        {
            "ContainerPath": "/var/zenoss",
            "Owner": "zenoss:zenoss",
            "Permission": "0775",
            "ResourcePath": "var-zenpacks"
        }
    ]
}
