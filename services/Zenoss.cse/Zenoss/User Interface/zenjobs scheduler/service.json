{
    "CPUCommitment": 1,
    "Command": "/opt/zenoss/bin/zenjobs beat -S Products.Jobber.scheduler.ZenJobsScheduler",
    "ConfigFiles": {
        "/opt/zenoss/etc/global.conf": {
            "Content": "# Generated by serviced\n{{range $k,$v:=contextFilter . \"global.conf.\"}}{{$k}} {{$v}}\n{{end}}",
            "Filename": "/opt/zenoss/etc/global.conf",
            "Owner": "zenoss:zenoss",
            "Permissions": "660"
        },
        "/opt/zenoss/etc/zenjobs.conf": {
            "Content": "{{(index (child (parent .) \"zenjobs\").ConfigFiles \"/opt/zenoss/etc/zenjobs.conf\").Content}}",
            "FileName": "/opt/zenoss/etc/zenjobs.conf",
            "Owner": "zenoss:zenoss",
            "Permissions": "0664"
        },
        "/opt/zenoss/etc/zenjobs_log_levels.conf": {
            "FileName": "/opt/zenoss/etc/zenjobs_log_levels.conf",
            "Owner": "zenoss:zenoss",
            "Permissions": "0664"
        },
        "/opt/zenoss/etc/zenjobs_schedules.yaml": {
            "FileName": "/opt/zenoss/etc/zenjobs_schedules.yaml",
            "Owner": "zenoss:zenoss",
            "Permissions": "0664"
        }
    },
    "Description": "Submits jobs to zenjobs according to a schedule",
    "Endpoints": [
        {
            "Application": "rabbitmq.*",
            "Name": "rabbitmq",
            "Purpose": "import"
        },
        {
            "Application": "redis",
            "Name": "redis",
            "PortNumber": 6379,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "controlplane_consumer",
            "Name": "controlplane_consumer",
            "PortNumber": 8444,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "zep",
            "Name": "zep",
            "PortNumber": 8084,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "zproxy",
            "Name": "zproxy",
            "PortNumber": 8080,
            "Protocol": "tcp",
            "Purpose": "import"
        }
    ],
    "HealthChecks": {
        "rabbit_answering": {
            "Interval": 10.0,
            "Script": "/opt/zenoss/bin/healthchecks/rabbit_answering {{(getContext . \"global.conf.amqpuser\")}} {{(getContext . \"global.conf.amqppassword\")}}"
        },
        "redis_answering": {
            "Interval": 5.0,
            "Script": "/opt/zenoss/bin/healthchecks/redis_answering"
        },
        "running": {
            "Interval": 5.0,
            "Script": "pgrep -fu zenoss zenjobs > /dev/null"
        }
    },
    "ImageID": "zenoss/zenoss5x",
    "Instances": {
        "default": 1,
        "max": 1,
        "min": 1
    },
    "Launch": "auto",
    "LogConfigs": [
        {
            "filters": [
                "pythondaemon"
            ],
            "path": "/opt/zenoss/log/zenjobs_scheduler.log",
            "type": "zenjobs-scheduler"
        }
    ],
    "Name": "zenjobs scheduler",
    "Prereqs": [],
    "RAMCommitment": "1G",
    "RunAs": "zenoss",
    "Services": [],
    "Tags": [
        "daemon"
    ],
    "Volumes": [
        {
            "#####": "drwxr-xr-x 2 zenoss zenoss 4096 Feb  6 19:11 /opt/zenoss/log/jobs",
            "ContainerPath": "/opt/zenoss/log/jobs",
            "Owner": "zenoss:zenoss",
            "Permission": "0755",
            "ResourcePath": "zenjobs"
        },
        {
            "#####": "drwxrwxr-x 3 zenoss zenoss 4096 Jul 18 11:20 /opt/zenoss/var/ext",
            "ContainerPath": "/opt/zenoss/var/ext",
            "Owner": "zenoss:zenoss",
            "Permission": "0775",
            "ResourcePath": "zenoss-var-ext"
        },
        {
            "ContainerPath": "/home/zenoss/.ssh",
            "Owner": "zenoss:zenoss",
            "Permission": "0755",
            "ResourcePath": ".ssh",
            "Type": ""
        },
        {
            "ContainerPath": "/var/zenoss",
            "Owner": "zenoss:zenoss",
            "Permission": "0775",
            "ResourcePath": "var-zenpacks"
        }
    ]
}
