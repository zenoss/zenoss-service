{
    "CPUCommitment": 1,
    "Command": "/usr/bin/redis-server /etc/redis.conf",
    "Description": "Redis server",
    "EmergencyShutdownLevel": 2,
    "Endpoints": [
        {
            "Application": "redis",
            "Name": "redis",
            "PortNumber": 6379,
            "Protocol": "tcp",
            "Purpose": "export"
        }
    ],
    "HealthChecks": {
        "metrics": {
            "Interval": 15,
            "Script": "/opt/zenoss/bin/healthchecks/redis/metrics"
        }
    },
    "ImageID": "zenoss/zenoss5x",
    "Instances": {
        "min": 1
    },
    "Launch": "auto",
    "LogConfigs": [
        {
            "path": "/var/log/redis/redis.log",
            "type": "redis"
        }
    ],
    "MonitoringProfile": {
        "GraphConfigs": [
            {
                "datapoints": [
                    {
                        "aggregator": "avg",
                        "fill": false,
                        "format": "%d",
                        "id": "total_commands_processed",
                        "legend": "Data Points",
                        "metric": "redis.total_commands_processed",
                        "metricSource": "redis",
                        "name": "Total Commands",
                        "rate": true,
                        "type": "line"
                    }
                ],
                "description": "Number of commands per second",
                "footer": false,
                "id": "totalcommands",
                "name": "Total Commands",
                "range": {
                    "end": "0s-ago",
                    "start": "1h-ago"
                },
                "returnset": "EXACT",
                "type": "line",
                "yAxisLabel": "Commands/s"
            }
        ],
        "MetricConfigs": [
            {
                "Description": "redis internal metrics",
                "ID": "redis",
                "Metrics": [
                    {
                        "Counter": true,
                        "Description": "Number of commands processed by redis.",
                        "ID": "redis.total_commands_processed",
                        "Name": "Total Commands",
                        "Unit": "Count"
                    }
                ],
                "Name": "redis internal metrics"
            }
        ]
    },
    "Name": "redis",
    "RAMCommitment": "256M",
    "StartLevel": 1,
    "Tags": [
        "daemon"
    ],
    "Volumes": [
        {
            "#####": "drwxr-xr-x 2 zenoss zenoss 4096 Feb  6 19:11 /var/lib/redis",
            "ContainerPath": "/var/lib/redis",
            "Owner": "root:root",
            "Permission": "0755",
            "ResourcePath": "redis"
        }
    ]
}