{
    "Actions": {
        "debug": "su - zenoss -c '${ZENHOME:-/opt/zenoss}/bin/{{.Name}} debug'",
        "stats": "su - zenoss -c '${ZENHOME:-/opt/zenoss}/bin/{{.Name}} stats'"
    },
    "CPUCommitment": 1,
    "Command": "/usr/bin/python3 -m http.server 8080",
    "ConfigFiles": {
        "/opt/zenoss/etc/global.conf": {
            "Content": "# Generated by serviced\n{{range $k,$v:=contextFilter . \"global.conf.\"}}{{$k}} {{$v}}\n{{end}}",
            "Filename": "/opt/zenoss/etc/global.conf",
            "Permissions": "660"
        },
        "/opt/zenoss/etc/zenhub.conf": {
            "FileName": "/opt/zenoss/etc/zenhub.conf",
            "Permissions": "0664"
        }
    },
    "Description": "Zenoss ZenHub",
    "Endpoints": [
        {
            "Application": "zodb_.*",
            "Name": "mariadb",
            "Purpose": "import"
        },
        {
            "Application": "memcached",
            "Name": "memcached",
            "PortNumber": 11211,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "redis",
            "Name": "redis",
            "PortNumber": 6379,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "rabbitmq.*",
            "Name": "rabbitmq",
            "Purpose": "import"
        },
        {
            "Application": "zep",
            "Name": "zep",
            "PortNumber": 8084,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "{{(parent .).Name}}_zenhubPB",
            "Name": "zenhub",
            "PortNumber": 8789,
            "Protocol": "tcp",
            "Purpose": "export"
        },
        {
            "Application": "{{(parent .).Name}}_zenhubXMLRpc",
            "Name": "zenhub_rpc",
            "PortNumber": 8081,
            "Protocol": "tcp",
            "Purpose": "export"
        },
        {
            "Application": "controlplane_consumer",
            "Name": "controlplane_consumer",
            "PortNumber": 8444,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "zproxy",
            "Name": "zproxy",
            "PortNumber": 8080,
            "Protocol": "tcp",
            "Purpose": "import"
        }
    ],
    "HealthChecks": {
        "central_query_answering": {
            "Interval": 10.0,
            "Script": "echo OK"
        },
        "mariadb_answering": {
            "Interval": 10.0,
            "Script": "echo OK"
        },
        "memcached_answering": {
            "Interval": 10.0,
            "Script": "echo OK"
        },
        "metric_consumer_answering": {
            "Interval": 10.0,
            "Script": "echo OK"
        },
        "rabbit_answering": {
            "Interval": 10.0,
            "Script": "echo OK"
        },
        "redis_answering": {
            "Interval": 5.0,
            "Script": "echo OK"
        },
        "rpc_listening": {
            "Comment": "TODO: exercise API, if possible",
            "Interval": 10.0,
            "Script": "echo OK"
        },
        "running": {
            "Interval": 5.0,
            "Script": "echo OK"
        },
        "stable_workers": {
            "Interval": 5.0,
            "Script": "echo OK"
        },
        "workers_running": {
            "Interval": 5.0,
            "Script": "echo OK"
        },
        "zep_answering": {
            "Interval": 10.0,
            "Script": "echo OK"
        }
    },
    "ImageID": "zenoss/ubuntu:wget",
    "Instances": {
        "min": 1
    },
    "Launch": "auto",
    "LogConfigs": [
        {
            "path": "/opt/zenoss/log/zenhub.log",
            "type": "zenhub"
        }
    ],
    "Name": "zenhub",
    "Prereqs": [],
    "RAMCommitment": "1G",
    "Services": [],
    "Tags": [
        "daemon",
        "hub"
    ]
}
